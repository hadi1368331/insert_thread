#define btn1_PORT                                gpioPortA
#define btn1_PIN                                 3

#define DataReady_PORT                           gpioPortC
#define DataReady_PIN                            9

#define LEDRed_PORT                              gpioPortF
#define LEDRed_PIN                               5

#define LEDGreen_PORT                            gpioPortF
#define LEDGreen_PIN                             6

#define CLC_PORT                                 gpioPortF
#define CLC_PIN                                  7

